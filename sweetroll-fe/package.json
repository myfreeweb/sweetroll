{
  "name": "sweetroll-fe",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "assets": "rm -rf dist.new; broccoli build dist.new && mkdir -p dist && find dist \\( -name '*.br' -or -name '*.gz' \\) -delete && rsync -au --inplace --remove-source-files dist.new/ dist && rm -rf dist.new && echo '==== Built assets'",
    "gzassets": "find dist \\( -name '*.css' -or -name '*.js' -or -name '*.svg' -or -name '*.html' \\) | parallel zopfli",
    "brassets": "find dist \\( -name '*.css' -or -name '*.js' -or -name '*.svg' -or -name '*.html' \\) | parallel brotli -f",
    "prod-assets": "npm run assets && npm run gzassets && npm run brassets",
    "devserve": "prok"
  },
  "processes": {
    "app": "DEBUG=*,-koa-mount,-css-color-function:parse,-dynamic-asset-rev IS_PROXIED=1 RELAX_CSP=1 INDIEAUTH_ENDPOINT=/login/test WEBSUB_HUB=https://httpbin.org/post MICRO_PANEL_ROOT=/micro-panel node-dev --harmony index.js",
    "re-asset": "chokidar 'assets/**/*' 'Brocfile.js' --initial -p -c 'SKIP_COMPRESSION=1 npm run assets'"
  },
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.0.3",
    "autoprefixer": "^9.0.1",
    "broccoli": "^1.1.4",
    "broccoli-cli": "^1.0.0",
    "broccoli-concat": "^3.5.1",
    "broccoli-config-replace": "^1.1.2",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-postcss": "^3.5.3",
    "broccoli-pug-render": "^2.0.0",
    "broccoli-source-map": "^0.2.4",
    "broccoli-static-asset-rev": "^0.1.1",
    "broccoli-svgstore": "^0.4.2",
    "chokidar-cli": "^1.2.0",
    "cssnano": "^4.0.4",
    "font-awesome-svg-png": "^1.2.2",
    "indieweb-components": "^0.1.3",
    "localforage": "^1.7.2",
    "node-dev": "^3.1.3",
    "normalize-opentype.css": "^0.2.4",
    "parallel": "^1.2.0",
    "postcss": "^7.0.1",
    "postcss-flexbugs-fixes": "^4.0.0",
    "postcss-nesting": "^6.0.0",
    "postcss-responsive-type": "^1.0.0",

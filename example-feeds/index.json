{
  "type": ["h-x-dynamic-feed"],
  "properties": {
    "url": ["https://unrelenting.technology/"],
    "name": ["Home"],
    "site-settings": [
      {
        "site-name": "unrelenting.technology"
      }
    ],
    "site-css": [
      ":root { --text-color: red; }"
    ],
    "feed-settings": [
      {
        "show-in-nav": true,
        "show-in-post": false,
        "order-in-nav": 0
      }
    ],
    "filter": [
      { "category": ["_notes"] },
      { "category": ["_articles"] },
      { "category": ["_reposts"] },
      { "index-display": ["show"] }
    ],
    "unfilter": [
      { "client-id": [ "https://micropub.rocks/" ] },
      { "index-display": ["hide"] }
    ],
    "author": [
      {
        "type": ["h-card"],
        "properties": {
          "note": [
            { "html": "Author bio. Include rel=me links for IndieAuth here (or certificate fingerprints for IndieCert or whatever)." }
          ]
        }
      }
    ]
  }
}
